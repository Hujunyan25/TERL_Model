   <string notr="true">/* ===== 全局字体和背景 ===== */
QMainWindow, QDialog {
    background-color: #f0f2f5; /* 非常浅的灰色背景，比纯白更有质感 */
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;, &quot;Helvetica Neue&quot;, sans-serif; /* 使用更现代的字体 */
}

/* ===== 视频显示区 ===== */
#imgDisplayLabel {
    background-color: #ffffff; /* 纯白背景 */
    border: 1px solid #dcdfe6; /* 浅灰色边框 */
    border-radius: 8px; /* 圆角可以大一点，显得更柔和 */
}

/* ===== 控制面板 (QGroupBox) ===== */
QGroupBox {
    background-color: #ffffff; /* 面板背景也是纯白 */
    border: 1px solid #dcdfe6;
    border-radius: 8px;
    margin-top: 25px; /* 给标题留出足够空间 */
    font-size: 11pt; /* 字体可以稍微大一点 */
}

/* 控制面板的标题样式 - 标签(Chip)版 */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    margin-left: 15px;
    padding: 4px 12px; /* 上下4px, 左右12px的内边距 */
    
    /* 关键部分在这里 */
    background-color: #e8eef8; /* 一个非常淡的蓝色，与主题呼应 */
    border-radius: 4px; /* 轻微的圆角 */
    color: #409eff; /* 文字颜色使用主题的主蓝色 */
    font-weight: bold;
    font-size: 10pt; /* 可以稍微调整字号 */
}

/* ===== 面板内的控件 ===== */

/* 标签样式 (QLabel) */
QGroupBox QLabel {
    color: #606266; /* 次要文字用灰色 */
    background-color: transparent;
    font-size: 10pt;
}

/* 输入框样式 (QLineEdit) */
QGroupBox QLineEdit {
    background-color: #fdfdfe;
    color: #303133; /* 主要文字用深灰色 */
    border: 1px solid #dcdfe6;
    border-radius: 4px;
    padding: 6px;
    font-size: 10pt;
}
QGroupBox QLineEdit:focus {
    border: 1px solid #409eff; /* 焦点颜色使用明亮的蓝色 */
    background-color: #ffffff;
}
QGroupBox QLineEdit:disabled {
    background-color: #f5f7fa; /* 禁用状态的颜色 */
    color: #c0c4cc;
}


/* ===== 按钮样式 (QPushButton) ===== */

/* 默认按钮样式 (蓝色) */
QGroupBox QPushButton {
    background-color: #409eff; /* 主按钮颜色 - 亮蓝色 */
    color: white;
    border: none;
    padding: 8px 20px;
    border-radius: 4px;
    font-size: 10pt;
    font-weight: 500;
}
QGroupBox QPushButton:hover {
    background-color: #66b1ff; /* 悬停时更亮 */
}
QGroupBox QPushButton:pressed {
    background-color: #3a8ee6; /* 按下时更深 */
}


/* &quot;继续&quot; 按钮的特定样式 (绿色) */
#Continue_Button {
    background-color: #67c23a; /* 成功的绿色 */
    color: white;
    border: none;
}
#Continue_Button:hover {
    background-color: #85ce61;
}
#Continue_Button:pressed {
    background-color: #5daf34;
}


/* &quot;暂停&quot; 按钮的特定样式 (红色) */
#Stop_Button {
    background-color: #f56c6c; /* 警告的红色 */
    color: white;
    border: none;
}
#Stop_Button:hover {
    background-color: #f78989;
}
#Stop_Button:pressed {
    background-color: #f35252;
}</string>
